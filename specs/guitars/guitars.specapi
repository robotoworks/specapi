package org.specapi.test

api TestAPI http://api.specapi.org {
    get getGuitar /guitars/id:long {
        response Guitar
    }
    
    post createGuitar /guitars {
        request Guitar
    }
    
    delete deleteGuitar /guitars {
        response Guitar
    }
}

entity Guitar {
    name:string,
    model:string,
    manufacturer:Manufacturer,
    type:GuitarType
}

entity Manufacturer {
    name:string
}

enum GuitarType {
    acoustic,
    electric
}